-include $(TOPDIR)/Make.defs

# uROS-publisher built-in application info

CONFIG_UROS_EXAMPLES_PUBLISHER_PRIORITY ?= SCHED_PRIORITY_DEFAULT
CONFIG_UROS_EXAMPLES_PUBLISHER_STACKSIZE ?= 65000

APPNAME = publisher
PRIORITY = $(CONFIG_UROS_EXAMPLES_PUBLISHER_PRIORITY)
STACKSIZE = $(CONFIG_UROS_EXAMPLES_PUBLISHER_STACKSIZE)

# uROS-publisher Example

ASRCS =
CSRCS =
MAINSRC = publisher_main.c 

CONFIG_UROS_EXAMPLES_PUBLISHER_PROGNAME ?= publisher$(EXEEXT)
PROGNAME = $(CONFIG_UROS_EXAMPLES_PUBLISHER_PROGNAME)
UROSDIR = $(shell find $(APPDIR)/../uros_ws/install -type d -name include)
CFLAGS += ${shell $(INCDIR) $(INCDIROPT) "$(CC)" "$(UROSDIR)"} -std=c99

MODULE = CONFIG_UROS_EXAMPLES_PUBLISHER

include $(APPDIR)/Application.mk
